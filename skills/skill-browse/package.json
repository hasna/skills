{
  "name": "skill-browse",
  "version": "1.0.0",
  "description": "MCP Browse server for AI agents - Browser automation using Browser-Use Cloud API",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "skill-browse": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "sse": "bun run src/sse-server.ts",
    "sse:dev": "bun run --watch src/sse-server.ts"
  },
  "keywords": [
    "browser",
    "automation",
    "scraping",
    "web",
    "ai-agent",
    "browser-use",
    "mcp"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasnaxyz/skill-browse.git"
  }
}
