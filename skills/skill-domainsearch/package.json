{
  "name": "@hasnaxyz/skill-domainsearch",
  "version": "0.1.2",
  "type": "module",
  "description": "CLI tool and HTTP API for domain search and availability checking via GoDaddy/Brandsight API",
  "private": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "restricted"
  },
  "bin": {
    "skill-domainsearch": "./bin/cli.ts"
  },
  "files": [
    "bin",
    "src",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasnaxyz/skill-domainsearch.git"
  },
  "homepage": "https://github.com/hasnaxyz/skill-domainsearch#readme",
  "bugs": {
    "url": "https://github.com/hasnaxyz/skill-domainsearch/issues"
  },
  "scripts": {
    "dev": "bun run --watch src/server/index.ts",
    "start": "bun run src/server/index.ts",
    "cli": "bun run bin/cli.ts"
  },
  "dependencies": {
    "commander": "^13.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "typescript": "^5.7.2"
  },
